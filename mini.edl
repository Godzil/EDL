
DECLARE output[36];

DECLARE input[36] ALIAS $AF010010F;

HANDLER TEST
{
	DECLARE hexCapture[36] ALIAS bin[1]:some[35];
	DECLARE upshift[36] ALIAS remaining[35]:%0;

	STATES first,a,b,c,d,e,f,g,h			# copy input constant to output first time this function is called assuming its called 9 times
	{
		STATE first
		{
			output <- input;
		}
	}

	hexCapture <- output;

	IF bin == 0
	{
		DEBUG_TRACE "0";
	}
	IF bin == 1
	{
		DEBUG_TRACE "1";
	}

	remaining <- some;
	output <- upshift;
}
